{% set links = ['Home', 'About', 'Services', 'Testimonials'] %}
{% set list_items %}
  {% for link in links %}
    {% set link %}
      {% include '@llds/llds-link/link.twig' with {
        attributes: {
          href: '#',
        },
        content: link
      } only %}
    {% endset %}
    {% include '@llds/llds-list-item/list-item.twig' with {
      content: link,
    } only %}
  {% endfor %}
{% endset %}

{% set list %}
  {% include '@llds/llds-list/list.twig' with {
    content: list_items,
  } only %}
{% endset %}

{% include '@llds/llds-menu/menu.twig' with {
  content: list,
} only %}